{
  "name": "balise/deepblue-installer",
  "type": "composer-plugin",
  "license": "MIT",
  "description": "A Wordpress Composer library installer",
  "keywords": [
    "WordPress"
  ],
  "autoload": {
    "psr-4": {
      "Balise\\DeepBlueInstaller\\": "src"
    }
  },
  "authors": [
    {
      "name": "Kyle Robinson Young",
      "email": "kyle@dontkry.com",
      "homepage": "https://github.com/shama"
    },
    {
      "name": "Simon Mongeau",
      "email": "simon@balise.ca"
    }
  ],
  "require": {
    "vlucas/phpdotenv": "^2.0.1",
    "composer-plugin-api": "^1.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-disable": "wordpress",
    "class": "Balise\\DeepblueInstaller\\Plugin" 
  }
}
